{
  "name": "findaneighbor",
  "version": "0.0.1",
  "main": "index.html",
  "scripts": {
    "start": "MODE=development parcel serve src/index.html --port 1337",
    "build": "MODE=production parcel build src/index.html --no-scope-hoist",
    "stage": "now && now rm findaneighbor -ys",
    "prod": "now --prod && now rm findaneighbor -ys"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/tehpsalmist/findaneighbor.git"
  },
  "keywords": [
    "parcel",
    "react",
    "tailwindcss",
    "template"
  ],
  "author": "Ben Steward <bstewardcodes@gmail.com> (https://tehpsalmist.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tehpsalmist/findaneighbor/issues"
  },
  "homepage": "https://github.com/tehpsalmist/findaneighbor#readme",
  "devDependencies": {
    "autoprefixer": "^10.2.5",
    "parcel": "^2.8.1",
    "process": "^0.11.10",
    "tailwindcss": "^2.2.19"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "formik": "^2.1.4",
    "graphql-hooks": "^5.1.1",
    "graphql-hooks-memcache": "^2.1.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0"
  },
  "app": "public/index.html",
  "targets": {
    "main": false,
    "app": {
      "engines": {
        "browsers": "Chrome 70"
      }
    }
  }
}
